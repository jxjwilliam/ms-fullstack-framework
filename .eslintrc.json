{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "settings": {
    "import/extensions": [".js", ".jsx"]
  },
  "rules": {
    "prettier/prettier": 2,
    "class-methods-use-this": 0,
    "func-names": 0,
    "no-debugger": 0,
    "no-alert": 0,
    "no-unused-vars": 1,
    "no-shadow": 0,
    "no-process-exit": 0,
    "no-multiple-empty-lines": 1,
    "no-var": 2,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0
  }
}
