{
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-fragments": 0,
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": 0,
    "no-return-assign": 0,
    "react/no-danger": 0,
    "no-nested-ternary": 0
  }
}
